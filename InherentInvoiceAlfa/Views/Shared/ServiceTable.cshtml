@model ServiceVM[]


<table class="table table-striped ">
    <thead> <tr> <th>#</th> <th>Tjänst/vara</th> <th>Antal</th> <th>Pris</th><th>Radera</th> </tr> </thead>
    <tbody>
        @for(int i = 0; i < Model.Length; i++)
            {
            <tr>
                <th scope=row>@(i+1)</th>
                <td>@Model[i].Label</td>
                <td>@Model[i].Amount</td>
                <td>@Model[i].Price</td>
                <td><input type="button" value="Radera" class="btn-danger delete-service" serviceID="@i"></td>
            </tr>
        }

       </tbody>
</table>
